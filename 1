#include <iostream>
#include <cmath>
#include <vector>
template<class T>
T sign(T x)
{
    return x < 0? -1 : x == 0? 0: 1;
}
struct Point{
    double x;
    double y;
};
std::vector <Point> terribleTrigonometry(unsigned  int n){
    std::vector <Point> points;
    const double PI = 3.141592653589793;
    double alpha = 2 * PI / n;
    double alpha_;
    for (int i = 0; i < n; i ++){
        Point p;
        alpha_ = alpha * i;
        p.x = sqrt(1 - (sin(alpha_)*sin(alpha_)));
        p.y = sqrt(1 - (p.x*p.x));
        auto sign_sin = sign(sin(alpha_));
        if (p.y != 0)
        p.y = p.y * sign_sin;
        auto sign_cos = sign (cos(alpha_));
        if (p.x != 0)
        p.x = p.x * sign_cos;
        points.push_back(p);
    }
    for (auto i : points){
        std::cout << i.x << " " << i.y << std::endl;
    }
    return points;
}
int main(){
    unsigned int n;
    std::cin >> n;
    terribleTrigonometry(n);
}
